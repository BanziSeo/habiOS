{
  "name": "habios",
  "private": true,
  "version": "0.8.15",
  "description": "habiOS - Professional Trading Journal Application",
  "author": "HabiOS",
  "type": "module",
  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BanziSeo/habiOS.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite",
    "build": "tsc -b && vite build",
    "build:electron": "npm run build && electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:prod": "vite build && set NODE_ENV=production && electron .",
    "test:prod:cross": "vite build && cross-env NODE_ENV=production electron .",
    "build:quick": "npm run build && electron-builder --win --dir",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-table": "^8.20.5",
    "adm-zip": "^0.5.16",
    "antd": "5.19.4",
    "archiver": "^7.0.1",
    "better-sqlite3": "^11.3.0",
    "chart-0714": "^1.7.6",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.6.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "immer": "^10.1.1",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "1.4.4",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/archiver": "^6.0.3",
    "@types/better-sqlite3": "^7.6.11",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-grid-layout": "^1.3.5",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "electron": "^31.7.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^3.2.4"
  }
}
